
@{
    ViewBag.Title = "UserIndex";
}
<div class="jumbotron">
    <h1>Dare</h1>
    <p class="lead"></p>
    <p>Welcome to Dare.</p>
    </div>

    @*Facebook Poster*@
    <h3>Post to Facebook</h3>
    <div class="col-md-offset-2">
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript"></script>
        <div id="fb-root">
        </div>
        <script>
            window.fbAsyncInit = function () {
                FB.init({
                    appId: '105120446672460', status: true, cookie: true,
                    xfbml: true
                });
            };
            (function () {
                var e = document.createElement('script'); e.async = true;
                e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
                document.getElementById('fb-root').appendChild(e);
            }());
        </script>
        <script type="text/javascript">
            $(document).ready(function () {
                $('#share_button').click(function (e) {
                    e.preventDefault();
                    FB.ui(
                    {
                        method: 'feed',
                        name: 'Dare',
                        link: 'http://wwwc-sharpcorner.com/authors/raj1979/raj-kumar.aspx',
                        picture: 'http://www.c-sharpcorner.com/UploadFile/AuthorImage/raj1979.gif',
                        caption: '',
                        description: '',
                        message: ''
                    });
                });
            });
        </script>
    </div>
    <input type="button" id="share_button" value="Share" />


